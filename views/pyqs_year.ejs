<%- include('partials/header') %>
<div class="main-container text-center space-y-6">
  <h2 class="text-2xl font-semibold"><%= year %> Year - PYQs</h2>
  <div class="flex justify-center gap-4">
    <% semesters[year].forEach(sem => { %>
      <a href="/pyqs/<%= year %>/<%= sem %>" class="px-6 py-3 bg-blue-700 hover:bg-blue-600 rounded-lg text-white"><%= sem.toUpperCase() %></a>
    <% }) %>
  </div>
</div>
<%- include('partials/footer') %>